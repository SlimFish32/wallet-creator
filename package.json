{
  "name": "@slimfish/wallet-creator",
  "version": "1.1.1",
  "description": "Small utility to create multiple wallets with ease",
  "author": "Slim Fish <SlimFish32@protonmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "gts clean && tsc",
    "start": "node ./dist/index.js",
    "fix": "gts fix",
    "publish": "npm publish dist/  --access=public",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "prepublish": "npm run compile && npm run prepare-package",
    "prepare-package": "node ./dist/setup-package.js && rimraf ./dist/setup-package.js"
  },
  "keywords": [
    "web3",
    "crypto",
    "bsc",
    "tool",
    "crypto-wallet-tool"
  ],
  "main": "./index.js",
  "bin": {
    "wallet-creator": "./index.js"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "bip39": "^3.0.4",
    "bn.js": "^5.2.0",
    "ethers": "^5.6.2",
    "progress": "^2.0.3",
    "prompts": "^2.4.2",
    "web3": "^1.7.1",
    "web3-core": "^1.7.1",
    "web3-eth-contract": "^1.7.1",
    "web3-utils": "^1.7.1"
  },
  "type": "commonjs",
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/node": "^14.11.2",
    "@types/progress": "^2.0.5",
    "copyfiles": "^2.4.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "gts": "^3.1.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3 && <= 4.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SlimFish32/wallet-creator.git"
  }
}
